# AI Model Validation Platform - Environment Configuration

# Backend API Configuration
REACT_APP_API_URL=http://localhost:8000

# WebSocket Configuration  
REACT_APP_WS_URL=ws://localhost:8000

# Development Settings
REACT_APP_DEBUG=true
REACT_APP_LOG_LEVEL=info

# Feature Flags
REACT_APP_ENABLE_GROUND_TRUTH=true
REACT_APP_ENABLE_REAL_TIME=true
REACT_APP_ENABLE_VIDEO_ANNOTATIONS=true

# Video Configuration
REACT_APP_MAX_VIDEO_SIZE=100MB
REACT_APP_SUPPORTED_VIDEO_FORMATS=mp4,avi,mov,mkv

# API Timeouts (in milliseconds)
REACT_APP_API_TIMEOUT=30000
REACT_APP_WS_TIMEOUT=20000

# Production Override (uncomment for production)
# REACT_APP_API_URL=https://your-domain.com/api
# REACT_APP_WS_URL=wss://your-domain.com