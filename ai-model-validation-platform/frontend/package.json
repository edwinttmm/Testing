  "scripts": {
    "start": "PORT=3000 craco start",
    "start:windows": "set PORT=3000 && craco start",
    "build": "craco build",
    "build:windows": "set NODE_ENV=production && craco build",
    "build:analyze": "ANALYZE=true craco build",
    "test": "craco test",
    "test:coverage": "craco test --coverage --watchAll=false",
    "test:debug": "craco test --verbose",
    "sparc:tdd": "npm run test:coverage && npm run build",
    "gpu:detect": "node -e \"console.log(require('./src/utils/gpu-detection.js').detectGPU())\" || echo 'CPU-only mode'",
    "postinstall": "npm run gpu:detect",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
