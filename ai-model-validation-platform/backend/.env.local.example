# Backend Local Override Configuration
# Copy this file to .env.local and customize as needed
# This file takes precedence over .env.development and .env.production
# Never commit .env.local to version control!

# Database Configuration (uncomment to override)
# DATABASE_URL=sqlite:///./custom_database.db
# DATABASE_ECHO=true

# Server Configuration (uncomment to override)
# API_HOST=0.0.0.0
# API_PORT=8080
# SOCKETIO_PORT=8081

# Custom CORS Origins (uncomment to override)
# CORS_ORIGINS=["http://localhost:3001", "http://192.168.1.100:3000"]

# Debug Settings (uncomment to override)
# DEBUG=true
# LOG_LEVEL=DEBUG
# ENABLE_SWAGGER_UI=true

# Custom Upload Settings (uncomment to override)
# MAX_UPLOAD_SIZE_MB=200
# UPLOAD_DIRECTORY=./custom_uploads

# Performance Settings (uncomment to override)
# ENABLE_GPU=false
# GPU_MEMORY_LIMIT=1024

# Security (uncomment for custom secret)
# SECRET_KEY=your-custom-secret-key-here

# Feature Flags (uncomment to override)
# ENABLE_MOCK_DATA=true
# ENABLE_RATE_LIMITING=false